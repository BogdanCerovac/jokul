    "dependencies": {
        "@fremtind/jkl-text-input": "^0.0.1"
    },
    "peerDependencies": {
        "@types/react": "^16.8.17",
        "@types/react-dom": "^16.8.4",
        "react": "^16.8.6",
        "react-dom": "^16.8.6"
    }
