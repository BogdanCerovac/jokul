{
    "name": "@fremtind/jkl-radio-button",
    "version": "0.0.1",
    "description": "Radio button fro the JÃ¸kul design system",
    "keywords": [
        "interface",
        "radio",
        "form",
        "form-field"
    ],
    "license": "MIT",
    "main": "build/css/radio-button.css",
    "directories": {
        "lib": "build"
    },
    "files": [
        "build"
    ],
    "scripts": {
        "build": "yarn build:style",
        "build:style": "yarn build:scss && yarn build:css",
        "prebuild:scss": "mkdirp -p build && mkdirp build/scss",
        "build:scss": "ncp src build/scss",
        "build:css": "node-sass src -o build/css --importer='../../node_modules/node-sass-tilde-importer'",
        "test": "echo \"Error: run tests from root\" && exit 1",
        "dev": "parcel src/index.html --open"
    }
}
