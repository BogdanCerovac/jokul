{
    "name": "@fremtind/jkl-message-box-react",
    "version": "0.1.0",
    "publishConfig": {
        "access": "public"
    },
    "description": "React wrapper for message box",
    "keywords": [
        "messagebox"
    ],
    "license": "MIT",
    "types": "./build/index.d.ts",
    "main": "./build/cjs/index.js",
    "module": "./build/esm/index.js",
    "browser": "./build/browser/index.js",
    "directories": {
        "lib": "build"
    },
    "files": [
        "build"
    ],
    "scripts": {
